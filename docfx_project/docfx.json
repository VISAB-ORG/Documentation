{
    "metadata": [
        {
            "src": [
                {
                    "files": [
                        "VISABConnector/*.csproj",
                        "VISABConnector.Unity/*.csproj"
                    ],
                    "src": "../../VISABConnector/src"
                }
            ],
            "exclude": [
                "../../VISABConnector/src/VISABConnector.Example/**"
            ],
            "dest": "api_visabconnector",
            "disableGitFeatures": false,
            "disableDefaultFilter": false
        }
    ],
    "build": {
        "xrefService": [
            "https://xref.docs.microsoft.com/query?uid={uid}"
        ],
        "content": [
            {
                "files": [
                    "**.md",
                    "**.yml"
                ]
            }
        ],
        "resource": [
            {
                "files": [
                    "images/**",
                    "misc/**",
                    "**.png",
                    "**.jpg"
                ]
            }
        ],
        "overwrite": [
            {
                "exclude": [
                    "obj/**",
                    "_site/**"
                ]
            }
        ],
        "dest": "_site",
        "globalMetadataFiles": [],
        "fileMetadataFiles": [],
        "template": [
            "default",
            "templates/material"
        ],
        "postProcessors": [],
        "markdownEngineName": "markdig",
        "noLangKeyword": false,
        "keepFileLink": false,
        "cleanupCacheHistory": false,
        "disableGitFeatures": false
    }
}